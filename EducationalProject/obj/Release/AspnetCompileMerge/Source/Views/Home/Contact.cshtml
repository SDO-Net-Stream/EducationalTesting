@using DotNetOpenAuth.OpenId.Extensions.AttributeExchange
@model IEnumerable<EducationalProject.Models.Contact>


@{
    ViewBag.Title = "Contact";
}

<hgroup class="title">
    <h1><center>Find Us</center></h1>
</hgroup>

<section class="contact">
    <div>
        <iframe class="gmap" src="https://mapsengine.google.com/map/u/0/embed?mid=zRXkQb75_AxQ.ksEkBIIzwNtg" width="640" height="480"></iframe>
    </div>

    @foreach (var item in Model)
    {
        <div class="contact_item">
            <span class="contact_h">@Html.Raw(@item.OfficeName)</span><br />
            @Html.Raw(@item.Address)<br />
            @item.Phone1<br />
            @item.Phone2<br />
            @item.Fax<br />

            <br />
        </div>

    }

</section>