<div ng-controller="app.views.test.list as vm">
    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="test in tests">
                <td>{{test.name}}</td>
                <td>
                    <a class="glyphicon glyphicon-pencil" ui-sref="test.edit({test: test.id})"></a>
                    <a class="glyphicon glyphicon-remove" ng-click="delete(test)"></a>
                </td>
            </tr>
        </tbody>
    </table>
    <button type="button" class="btn btn-primary" ng-click="create()">Create new test</button>
</div>
